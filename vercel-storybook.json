{
  "version": 2,
  "name": "storybook-ionic-storybook",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "storybook-static",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
